
(rules PCB 1u_complex_bus_mainboard
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 4609)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 27.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.0)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(D1-Pad1)" "Net-(D2-Pad2)" "Net-(R_Split_1_1-Pad2)" "Net-(R_Split_2_1-Pad2)" "Net-(R_Split_3_1-Pad2)" "Net-(R_Split_4_1-Pad2)" "Net-(C5-Pad2)" "/cv_bus1/LEDS_OUT_1"
    "/cv_bus2/LEDS_OUT_2" "Net-(C6-Pad2)" "Net-(C7-Pad2)" "Net-(C7-Pad1)" "/cv_bus4/LEDS_OUT_4" "Net-(C8-Pad2)" "/cv_bus1/IN_1_1" "/cv_bus1/IN_1_2"
    "/cv_bus1/IN_1_3" "/cv_bus1/IN_1_4" "/cv_bus1/OUT_1_1" "/cv_bus1/OUT_1_2" "/cv_bus1/OUT_1_3" "/cv_bus1/OUT_1_4" "/cv_bus1/LEDS_IN_1" "/cv_bus2/IN_2_1"
    "/cv_bus2/IN_2_2" "/cv_bus2/IN_2_3" "/cv_bus2/IN_2_4" "/cv_bus2/OUT_2_1" "/cv_bus2/OUT_2_2" "/cv_bus2/OUT_2_3" "/cv_bus2/OUT_2_4" "/cv_bus2/LEDS_IN_2"
    "/cv_bus3/IN_3_1" "/cv_bus3/IN_3_2" "/cv_bus3/IN_3_3" "/cv_bus3/IN_3_4" "/cv_bus3/OUT_3_1" "/cv_bus3/OUT_3_2" "/cv_bus3/OUT_3_3" "/cv_bus3/OUT_3_4"
    "/cv_bus3/LEDS_IN_3" "/cv_bus3/LEDS_OUT_3" "/cv_bus4/LEDS_IN_4" "/cv_bus4/OUT_4_4" "/cv_bus4/OUT_4_3" "/cv_bus4/OUT_4_2" "/cv_bus4/OUT_4_1" "/cv_bus4/IN_4_4"
    "/cv_bus4/IN_4_3" "/cv_bus4/IN_4_2" "/cv_bus4/IN_4_1" "Net-(R1-Pad1)" "Net-(R1-Pad2)" "Net-(R3-Pad1)" "Net-(R3-Pad2)" "Net-(R5-Pad2)"
    "Net-(R5-Pad1)" "Net-(R7-Pad2)" "Net-(R7-Pad1)" "Net-(U1-Pad13)" "Net-(U2-Pad13)" "Net-(U3-Pad13)" "Net-(U4-Pad13)" +12V
    GND "-12V"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)